RewriteEngine On

#global
RewriteRule ^index.html$ index.php
RewriteRule ^404.html$ index.php?page=404
RewriteRule ^page-(.*).html$ index.php?page=$1

#user
RewriteRule ^user.html$ index.php?page=user
RewriteRule ^user-(.*).html$ index.php?page=user&userID=$1
RewriteRule ^userLogout.html$ index.php?page=user_logout
RewriteRule ^userChangePassword.html$ index.php?page=user_changePassword

#post
RewriteRule ^post.html$ index.php?page=post
RewriteRule ^postEdit-(.*).html$ index.php?page=post&id=$1
RewriteRule ^postAdd.html$ index.php?page=post&id=dodaj

#menu
RewriteRule ^menu.html$ index.php?page=menu
RewriteRule ^menuAdd.html$ index.php?page=menu&type=add
RewriteRule ^menuDelete-(.*).html$ index.php?page=menu&id=$1&type=delete
RewriteRule ^menuEdit-(.*).html$ index.php?page=menu&id=$1&type=edit
RewriteRule ^menuPositionUp-(.*).html$ index.php?page=menu&id=$1&type=positionUp
RewriteRule ^menuPositionDown-(.*).html$ index.php?page=menu&id=$1&type=positionDown

#jumbotron
RewriteRule ^jumbotron.html$ index.php?page=jumbotron

#siteTemplate
RewriteRule ^siteTemplate.html$ index.php?page=fourcms_template
RewriteRule ^siteTemplateActive-(.*).html$ index.php?page=fourcms_template&template=$1

#config
RewriteRule ^config.html$ index.php?page=fourcms_config

#templateEdit
RewriteRule ^templateEdit.html$ index.php?page=template_edit

#userAdmin and permission
RewriteRule ^userAdmin.html$ index.php?page=fourcms_users
RewriteRule ^createNewUser.html$ index.php?page=fourcms_users_addUser
RewriteRule ^permission.html$ index.php?page=fourcms_permission
RewriteRule ^permissionEdit-(.*).html$ index.php?page=fourcms_permission&editID=$1
RewriteRule ^permissionAdd.html$ index.php?page=fourcms_permission&editID=addNew

#service
RewriteRule ^FrameworkLibrary.html$ index.php?page=fourframework_library
RewriteRule ^phpInfo.html$ index.php?page=fourframework_phpinfo

#service logs
RewriteRule ^logs.html$ index.php?page=fourframework_logs
RewriteRule ^logFile-(.*).html$ index.php?page=fourframework_logs&file=$1
RewriteRule ^logDelete-(.*).html$ index.php?page=fourframework_logs&delete=$1

#service module
RewriteRule ^FrameworkModule.html$ index.php?page=fourframework_module
RewriteRule ^FrameworkModuleInfo-(.*).html$ index.php?page=fourframework_module&type=info&name=$1
RewriteRule ^FrameworkModuleAP-(.*).html$ index.php?page=fourframework_module&type=adminpanel&modul=$1 [QSA,L]
RewriteRule ^FrameworkModuleDebug-(.*).html$ index.php?page=fourframework_module&type=debug&name=$1

#search
RewriteRule ^search.html$ index.php?page=searchMenu [QSA,L]